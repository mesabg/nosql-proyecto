{
  "name": "nosql-proyecto",
  "version": "1.0.0",
  "description": "Proyecto NoSQL - Semestre 8",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- src/server.js",
    "lint": "eslint src/",
    "build": "babel src -d dist",
    "preprod": "npm run build",
    "prod": "node dist/server.js"
  },
  "engines": {
    "node": "9.3.0",
    "npm": "5.6.0"
  },
  "keywords": [
    "NoSQL",
    "MongoDB",
    "ExpressJS",
    "Disney"
  ],
  "author": "Mois√©s Berenguer",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesabg/nosql-proyecto.git"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-inline-json": "^1.2.2",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "eslint": "^4.14.0",
    "eslint-config-zavatta": "^6.0.1",
    "eslint-plugin-import": "^2.8.0",
    "mongoose": "^5.0.0-rc1",
    "neo4j": "^2.0.0-RC2",
    "neo4j-driver": "^1.5.3"
  },
  "devDependencies": {
    "nodemon": "^1.14.7"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      [
        "inline-json",
        {
          "matchPattern": "config"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "zavatta",
    "rules": {
      "no-console": 0,
      "callback-return": 0
    }
  }
}
